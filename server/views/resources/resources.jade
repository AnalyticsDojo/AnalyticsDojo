extends ../layout
block content
    .row
        .col-xs-12.col-sm-10.col-sm-offset-1.col-md-6.col-md-offset-3

    .spacer
    .row
        .col-xs-12.col-sm-10.col-sm-offset-1.col-md-6.col-md-offset-3
            h2.text-center Technology
            hr
            table.table.link-table
                tr
                    td.text-center
                        i.fa.fa-windows
                    td
                        a(href='//studio.azureml.net', target='_blank') Microsoft Azure ML Studio
                tr
                    td.text-center
                        i.fa.fa-cubes
                    td
                        a(href='//docs.docker.com/engine/installation/', target='_blank') Get Docker
                tr
                    td.text-center

                        i.fa.fa-github
                    td
                        a(href="//github.com/AnalyticsDojo/materials", target='_blank') AnalyticsDojo Notebooks and Docker Image

    .spacer
        .row
            .col-xs-12.col-sm-10.col-sm-offset-1.col-md-6.col-md-offset-3
                h2.text-center Python Online Resources
                hr
                table.table.link-table
                    tr
                        td.text-center
                            i.fa.fa-windows
                        td
                            a(href='//learnpythonthehardway.org/book/', target='_blank') Learn Python the Hard Way
                    tr
                        td.text-center
                            i.fa.fa-cubes
                        td
                            a(href='//greenteapress.com/thinkpython2/) ', target='_blank') Think Python
                    tr
                        td.text-center

                            i.fa.fa-github
                        td
                            a(href="//opentechschool.github.io/python-data-intro/", target='_blank') Open Tech School Introduction to Data Processing with Python
    .spacer
        .row
            .col-xs-12.col-sm-10.col-sm-offset-1.col-md-6.col-md-offset-3
                h2.text-center R Online Resources
                hr
                table.table.link-table
                    tr
                        td.text-center
                            i.fa.fa-windows
                        td
                            a(href='//r4ds.had.co.nz', target='_blank') R for Data Science

                    tr
                        td.text-center
                            i.fa.fa-cubes
                        td
                            a(href='//www-bcf.usc.edu/~gareth/ISL/) ', target='_blank') An Introduction to Statistical Learning with Applications in R
                    tr
                        td.text-center

                            i.fa.fa-github
                        td
                            a(href="////cran.r-project.org/manuals.html", target='_blank') Introduction to R Manuals
        .spacer

    .row
        .col-xs-12.col-sm-10.col-sm-offset-1.col-md-6.col-md-offset-3
            h2.text-center Whom to contact for what
            table.table.table-stripe
                tr
                    td Support
                    td
                        a(href='mailto:jason@analyticsdojo.com') jason@analyticsdojo.com
                tr
                    td Get Involved
                    td
                        a(href='mailto:jason@analyticsdojo.com') jason@analyticsdojo.com



